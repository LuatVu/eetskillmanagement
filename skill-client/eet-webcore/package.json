{
  "name": "eet-webcore",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:core-demo",
    "start:core-demo": "ng serve --port 5555 --proxy-config ./projects/eet-core-demo/proxy.conf.json eet-core-demo",
    "start:core-net": "ng serve --port 8888 --proxy-config ./projects/eet-core-demo/proxy.net.conf.json --configuration development-net eet-core-demo",
    "start:core-java": "ng serve --port 9999 --proxy-config ./projects/eet-core-demo/proxy.java.conf.js --configuration development-java eet-core-demo",
    "build:net": "ng build eet-core-demo --configuration production-net --base-href /eet-core-net/",
    "build:java": "ng build eet-core-demo --configuration production-java --base-href /eetportal/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
