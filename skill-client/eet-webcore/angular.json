{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "core": {
      "projectType": "library",
      "root": "projects/core",
      "sourceRoot": "projects/core/src",
      "prefix": "eet",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/core/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/core/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/core/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/core/src/test.ts",
            "tsConfig": "projects/core/tsconfig.spec.json",
            "karmaConfig": "projects/core/karma.conf.js"
          }
        }
      }
    },
    "eet-core-demo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/eet-core-demo",
      "sourceRoot": "projects/eet-core-demo/src",
      "prefix": "eet",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/eet-core-demo",
            "index": "projects/eet-core-demo/src/index.html",
            "main": "projects/eet-core-demo/src/main.ts",
            "polyfills": "projects/eet-core-demo/src/polyfills.ts",
            "tsConfig": "projects/eet-core-demo/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/eet-core-demo/src/favicon.ico",
              "projects/eet-core-demo/src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/@bci-web-core/common-styles/img",
                "output": "/assets/img/"
              }
            ],
            "styles": [
              "./node_modules/@bosch/frontend.kit-npm/dist/frontend-kit.css",
              "./node_modules/bootstrap/dist/css/bootstrap-grid.min.css",
              "projects/eet-core-demo/src/styles.scss"
            ],
            "scripts": [
              "./node_modules/@bosch/frontend.kit-npm/dist/frontend-kit.js"
            ]
          },
          "configurations": {
            "production-net": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kb",
                  "maximumError": "30kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/eet-core-demo/src/environments/environment.ts",
                  "with": "projects/eet-core-demo/src/environments/net/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "production-java": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kb",
                  "maximumError": "30kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/eet-core-demo/src/environments/environment.ts",
                  "with": "projects/eet-core-demo/src/environments/java/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development-net": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": []
            },
            "development-java": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": []
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "eet-core-demo:build:production"
            },
            "development": {
              "browserTarget": "eet-core-demo:build:development"
            },
            "development-java": {
              "browserTarget": "eet-core-demo:build:development-java"
            },
            "development-net": {
              "browserTarget": "eet-core-demo:build:development-net"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "eet-core-demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/eet-core-demo/src/test.ts",
            "polyfills": "projects/eet-core-demo/src/polyfills.ts",
            "tsConfig": "projects/eet-core-demo/tsconfig.spec.json",
            "karmaConfig": "projects/eet-core-demo/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/eet-core-demo/src/favicon.ico",
              "projects/eet-core-demo/src/assets"
            ],
            "styles": ["projects/eet-core-demo/src/styles.scss"],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "core"
}
